{
	"name": "Getting Started Example",
	"description": "Build an Extension",
	"version": "1.0",
	"manifest_version": 3,
	"action": {
		"default_popup": "popup.html"
	},
	"content_scripts": [
		{
		"matches": [
			"<all_urls>"
		],
		"css": ["style.css"],
		"js": ["content.js"]
		}
	],
	"background": {
		"service_worker": "background.js"
	}
}